rules_version = '2';
service firebase.storage {
  match /b/{bucket}/o {
    match /crews {
    	match /{allPaths=**} {
      	allow read: if true;
      	allow write: if true;
      }
    }
    
    match /users {
    	match /{allPaths=**} {
      	allow read: if true;
      	allow write: if true;
      }
    }

    match /{allPaths=**} {
      allow read, write: if request.auth!=null;
    }
  }
}
